// prisma/schema.prisma
generator client {
  provider = "prisma-client-js" 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id            Int      @id @default(autoincrement())
  name          String
  databaseUrl   String
  neonProjectId String
  pineconeIndex String?
  createdAt     DateTime @default(now())
}
